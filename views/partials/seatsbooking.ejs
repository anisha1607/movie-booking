<% for(var i=0; i<6; i++) { %>
    <div class="row a2">
<%  for(var j=1; j<9; j++){ %>
<%      var s = String.fromCharCode(65 + i) + j.toString(); %>
<%      s = s.toString(); %>
<%      if(filledSeats.includes(s)) { %>
            <div class="seat occupied" name=<%= s %> id=<%= s %>><%= s %></div>
<%      } else { %>
            <div class="seat" name=<%= s %> id=<%= s %>><%= s %></div>
<%      } %>
<%  } %>
    </div>
<% } %>
